    <!-- begin::main-content -->
    <div class="main-content">

    
   

    <div class="row">
        <div class="col-md-12">
            <div class="card">
                <div class="card-body">
                    <button type="button" class="btn btn-dark btn-lg" data-toggle="modal" data-target="#exampleModalCenter">
                        <i class="fa fa-plus"></i> 
                      </button>
                      
                      <!-- Modal -->
                    <div class="modal fade" id="exampleModalCenter" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
                        
                        <div class="modal-dialog modal-dialog-centered" role="document">
                        
                         <div class="modal-content">
                            <div class="modal-header">
                              <h5 class="modal-title" id="exampleModalLongTitle">Create Room</h5>
                              <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                <span aria-hidden="true">&times;</span>
                              </button>
                            </div>
                            <div class="modal-body">
                                <div class="d-flex justify-content-between">
                                    <div class="container"> 
                                        <form action="/home" method="post">
                                            <input type="text" class="form-control" name="room" id="" placeholder="Room Name">
                                            <br/>
                                            <input type="text" class="form-control" name="description" id="" placeholder="Description">
                                            <br/>
                                            <button class="btn btn-secondary upload-btn" type="button">Upload File</button>
                                            <input type="file" class="btn btn-primary" name="upload" id="upload-input" style="display: none;"/> 
                                            
                                            <div class="modal-footer">
                                                <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                                                <button type="submit" class="btn btn-primary">Add Room</button>
                                              </div>
                                        </form>
                                        </div>
                                    
                                </div>
                            </div>
                           
                          </div>
                        
                        </div>
                    
                    </div>
                    
                      <script>
                        $(document).ready(function() {
                            $('.upload-btn').on('click',function(){
                                $('#upload-input').click();
                            });
                        
                            $('#upload-input').on('change', function(){
                                var uploadInput = $('#upload-input');
                        
                                if(uploadInput.val() != ''){
                                    var formData = new FormData();
                        
                                    formData.append('upload', uploadInput[0].files[0]);
                        
                                    $.ajax({
                                        url: '/uploadFile',
                                        type: 'POST',
                                        data: formData,
                                        processData: false,
                                        contentType: false,
                                        success: function(){
                                            uploadInput.val('');
                                        } 
                                    })
                                }
                            })
                        })
                        </script>
                    
                   
                    
                </div>
            </div>
            <% if(first.length > 0){ %>
            <div class="row">
                <div class="col-lg-12 col-md-12">
                   
                    <h5 style="text-transform: uppercase;">Room suggestions</h5>
                    <div class="slick-js">
                       
                        <% _.forEach(first, function(val){ %>
                        <div class="">
                            
                            <div class="card bg-secondary-gradient border-0" style="height: 140px;">
                                <div class="card-body text-center">
                                    <h4 class="font-weight-bold mb-3"><%= val.name %></h4>
                                    <h5 class="mb-0"><%= val.description %></h5>
                                    <a href="/room/<%= val.name%>" class="btn-sm btn-dark" style="float: right; margin-top: 20px; margin-left: 50px;"> <i data-feather="plus"></i></a>
                                </div>
                            </div>
                          
                        </div>
                   
                    <% }) %>   
                </div>
                 
                
                
            </div>
           
